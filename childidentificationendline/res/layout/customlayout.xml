                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="7. Nalidixic acid" >
                </RadioButton>

                <RadioButton
                    android:id="@+id/radioGroup208time1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="8. Doxycycline" >
                </RadioButton>

                <RadioButton
                    android:id="@+id/radioGroup209time1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="9. Betapen (penicillin)" >
