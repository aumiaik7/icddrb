                    android:layout_height="wrap_content"
                    android:text="4. Ciprofloxacin" >
                </RadioButton>

                <RadioButton
                    android:id="@+id/radioGroup205time1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="5. Erythromycin" >
                </RadioButton>

                <RadioButton
                    android:id="@+id/radioGroup206time1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="6. Azythromycin" >
                </RadioButton>

