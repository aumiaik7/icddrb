                android:layout_column="2"
                android:layout_gravity="center"
                android:layout_row="1"
                android:ems="10"
                android:inputType="number"
                android:maxLength="3"
                android:width="60dp" />

            <RadioGroup
                android:id="@+id/radioGroup46h2"
